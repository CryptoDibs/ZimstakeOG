    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;
    border-top-style: inset;
    text-align: center;
    background: transparent;
}

QProgressBar::chunk
{
    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;    
    width: 10px;
    margin: 0.5px;
}

