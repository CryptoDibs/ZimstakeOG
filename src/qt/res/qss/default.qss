QToolButton
{
    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;
	font-size:11px;
    border:none;
		padding:5px;
	
}

QToolButton::hover
{
    color: #ffffff;
    font-weight: normal;
    background-color:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 transparent, stop: 0.4 #e592a9, stop: 0.5 #e592a9, stop: 1.0 transparent); 
    border: none;

}

QToolButton::checked
{
    color: #EEEEEE;
    font-weight: normal;
    border-bottom:1px solid #610a2e;
    background-color:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 transparent, stop: 0.4 #970e48, stop: 0.5 #970e48, stop: 1.0 #610a2e); 
}

