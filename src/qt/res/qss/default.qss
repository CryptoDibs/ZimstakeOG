    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;
    background: #2C3236;
    border: 1px solid black;
    border-radius: 5px;
    padding: 1px;
}

QComboBox::editable
{
    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;
    background: #2C3236;
    border: 1px solid black;
    border-radius: 5px;
    padding: 1px;
}
QStatusBar::item{
border:none;
}
