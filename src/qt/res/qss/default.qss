QMainWindow
{
    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;
    background-image: url(:/images/v-back);
    background-position: top center;
}

QTableView
{
    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;
}

QFrame
{
    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;
    background: transparent;
    border:none;
}

QTreeWidget
{
    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;
}
QMainWindow::QToolBar
{
	background-color: white;    
	border:0px solid blue;
    spacing:35px;
}
QToolBar
{
	 
	border:0px solid blue;
    background-color: white;   
	spacing:35px;
	    background-image: url(:/images/m-back);

}

QTabWidget
{
    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;
    background-image: url(:/images/s-back);
}

QTabBar::tab
{
    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;
    background-image: url(:/images/button);
    border: 1px solid white;
    border-radius: 2px;
    padding: 3px;
}

QTabBar::tab:selected, QTabBar::tab:hover
{
    font-family: Arial, Helvetica, sans-serif;
    color: #ffffff;
    font-weight: normal;
    background-image: url(:/images/button);
}

